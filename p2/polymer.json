{
  "entrypoint": "index.html",
  "shell": "src/app-shell.html",
  "fragments": [
    "src/shared-styles.html",
    "src/common-resources.html",
    "src/list-view.html",
    "src/search-view.html",
    "src/item-view.html",
    "src/add-view.html",
    "src/edit-view.html",
    "src/vaadin-upload-style.html",
    "src/admin-view.html",
    "src/app-analytics.html",
    "src/notfound-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "robots.txt",
    "manifest.json",
    "firebase-messaging-sw.js",
    "bower_components/webcomponentsjs/*",
    "bower_components/moment/min/*"
  ],
  "lint": {
    "rules": ["polymer-2"],
    "ignoreWarnings": []
  },
  "builds": [
    {
      "preset": "es6-unbundled",
      "name": "default"
    }
  ]
}
