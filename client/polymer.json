{
  "entrypoint": "index.html",
  "shell": "src/app-shell.html",
  "fragments": [
    "src/app-icons.html",
    "src/shared-styles.html",
    "src/redux-store.html",
    "src/grid-view.html",
    "src/app-search.html",
    "src/detail-view.html",
    "src/item-view.html",
    "src/item-info.html",
    "src/item-edit.html",
    "src/form-view.html",
    "src/admin-shell.html",
    "src/error-404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "robots.txt",
    "manifest.json",
    "firebase-messaging-sw.js",
    "node_modules/redux/dist/redux.min.js",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ],
  "builds": [
    {
      "bundle": false,
      "addServiceWorker": true,
      "js": {
        "minify": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ]
}
